# Shannon Portrait - InfoCodec Environment Variables
# Copy this file to .env and fill in your values

# OpenRouter API Key for LLM integration
OPENROUTER_API_KEY=your-api-key-here

# Default settings (optional)
INFOCODEC_DEFAULT_METHOD=auto
INFOCODEC_QUALITY=1.0
INFOCODEC_ENABLE_CACHE=true

# Cache directory (optional)
INFOCODEC_CACHE_DIR=~/.infocodec/cache

# Logging level
INFOCODEC_LOG_LEVEL=INFO
